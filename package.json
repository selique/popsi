{
  "name": "popsi",
  "version": "0.1.0",
  "private": true,
  "author": "selique.tech",
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export -o build",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
    "migrate:dev": "npx prisma migrate dev --skip-generate"
  },
  "dependencies": {
    "@brazilian-utils/format-cpf": "^0.1.8",
    "@capacitor/android": "3.4.3",
    "@capacitor/camera": "^1.3.1",
    "@capacitor/cli": "3.4.3",
    "@capacitor/clipboard": "^1.0.8",
    "@capacitor/core": "3.4.3",
    "@capacitor/ios": "3.4.3",
    "@capacitor/share": "^1.1.2",
    "@capacitor/status-bar": "^1.0.8",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.0",
    "@ionic-native/social-sharing": "^5.36.0",
    "@ionic/cli": "6.19.1",
    "@ionic/pwa-elements": "^3.1.1",
    "@ionic/react": "^5.5.2",
    "@ionic/react-router": "^5.5.2",
    "@prisma/client": "^4.0.0",
    "@supabase/supabase-js": "^1.35.3",
    "@upstash/qstash": "^0.1.5",
    "chart.js": "^3.8.0",
    "classnames": "^2.2.6",
    "date-fns": "2.28.0",
    "dayjs": "^1.11.3",
    "es6-promise-plugin": "4.2.2",
    "ionicons": "^6.0.1",
    "lottie-react": "^2.3.1",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.31.2",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^5.2.0",
    "react-virtuoso": "^2.16.1",
    "styled-components": "5.3.3",
    "uuidv4": "^6.2.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.5",
    "copy-webpack-plugin": "^10.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "postcss": "^8.4.14",
    "prettier": "2.5.1",
    "prisma": "^4.0.0",
    "pullstate": "1.24.0",
    "react-use-gesture": "9.1.3",
    "reselect": "4.1.5",
    "tailwindcss": "^3.0.24"
  }
}
