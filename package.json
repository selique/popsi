{
  "name": "popsi",
  "version": "0.1.0",
  "private": true,
  "author": "Mois√©s Lopes Ferreira <moises.ferreira@planyourlife.tech> (@selique)",
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export -o build",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|jsx|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'"
  },
  "dependencies": {
    "@capacitor/android": "3.4.3",
    "@capacitor/cli": "3.4.3",
    "@capacitor/core": "3.4.3",
    "@capacitor/ios": "3.4.3",
    "@capacitor/status-bar": "1.0.8",
    "@ionic/react": "^6.0.7",
    "@ionic/react-router": "6.0.5",
    "autoprefixer": "10.4.4",
    "classnames": "2.3.1",
    "ionicons": "^6.0.1",
    "next": "12.0.10",
    "postcss": "8.4.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.3.0",
    "react-virtuoso": "2.6.0",
    "styled-components": "5.3.3",
    "tailwindcss": "3.0.19"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "commitizen": "4.2.4",
    "copy-webpack-plugin": "^10.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.5.1",
    "pullstate": "1.23.0",
    "react-use-gesture": "9.1.3",
    "reselect": "4.1.5"
  }
}
